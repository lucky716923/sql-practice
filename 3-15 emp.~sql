select * from emp;
select * from emp where comm is not null;
select * from emp where sal>1000;
select distinct job from emp;
select rowid from emp;
select rowid,emp.* from emp order by job;
delete from emp where rowid='AAAQ+jAAEAAAAAeAAM';
delete from emp where rowid='AAAQ+jAAEAAAAAeAAL' 
and rowid='AAAQ+jAAEAAAAAeAAA'
and rowid='AAAQ+jAAEAAAAAeAAK '
and rowid='AAAQ+jAAEAAAAAeAAD'
and rowid='AAAQ+jAAEAAAAAeAAG'
and rowid='AAAQ+jAAEAAAAAeAAE '
and rowid='AAAQ+jAAEAAAAAeAAC' and rowid='AAAQ+jAAEAAAAAeAAB';
select * from emp where comm is not null and sal>1000 order by sal ;
select rownum,emp.* from emp order by job;

